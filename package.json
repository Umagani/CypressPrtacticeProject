{
  "devDependencies": {
    "cypress": "^11.2.0"
  },
  "scripts": {
    "cypress:open": "cypress open --e2e --browser chrome",
    "cypress:run": "cypress run --e2e --browser chrome",
    "cypress:headless": "cypress run --spec 'cypress/e2e/**/**/*.js, cypress/e2e/**/*.js' --config videoUploadOnPasses=false",
    "cypress:recordFullScreen": "cross-env CYPRESS_NO_COMMAND_LOG1=1 cypress run --e2e --browser chrome --spec"
  },
  "dependencies": {
    "mocha": "^10.7.3"
  }
}